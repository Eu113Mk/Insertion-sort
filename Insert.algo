FUNCTION insertionSort(arr, n) 
VAR
        i, key, j : INTEGER
BEGIN

    set_instructions
END_FOR
    FOR i FROM 1 TO n DO
        key = arr[i]
        j = i - 1

        while (j >= 0 and arr[j] > key) 
         
            arr[j + 1] = arr[j]  
            j = j - 1 
        End_While  
        arr[j + 1] = key;
    END_FOR
END
